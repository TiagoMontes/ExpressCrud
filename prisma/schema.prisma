generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Movie {
  id      Int       @id @default(autoincrement())
  name    String    @unique
  year    String
  genres  Genre[]
}

model Genre {
  id        Int      @id @default(autoincrement())
  genero    String
  movies    Movie[]
}